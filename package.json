{
  "name": "coraline-client",
  "description": "Your favourite client package helper.",
  "version": "2024.6.12",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    "types": "./dist/esm/index.d.ts",
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js"
  },
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsc -w & node --watch .",
    "start": "node .",
    "lint": "eslint ./src",
    "build": "rimraf dist && npm run lint && tsc && npm run build:cjs",
    "build:cjs": "rollup ./dist/esm/index.js --file dist/index.js --format cjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "^54.0.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2"
  }
}
